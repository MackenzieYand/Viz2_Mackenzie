{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 400,
    "height": 300,
    "data": { "url": "https://raw.githubusercontent.com/MackenzieYand/Viz2_Mackenzie/main/data/aleague/teams_attendance.csv" }, 
    "mark": { "type": "bar" },
    "encoding": {
        "x": { "field": "Club_1", "type": "ordinal" },
        "y": { "aggregate": "sum", "field": "Total_1" },
        "color": { "field": "Year" }
    },
    "config": {
      "background": "#ffe342",
      "title": {
        "color": "black",
        "font": "'Roboto', sans-serif",
        "fontSize": 14
      },
      "legend": {
        "labelFont": "'Roboto', sans-serif",
        "labelColor": "black",
        "titleFont": "'Roboto', sans-serif",
        "titleColor": "black"
      }
    }
}